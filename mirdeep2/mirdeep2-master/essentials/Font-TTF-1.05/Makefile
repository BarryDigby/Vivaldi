# --- MakeMaker postamble section:
debsrc: dist
	rm -fr Font-TTF-1.05
	rm -fr libfont-ttf-perl
	rm -fr libfont-ttf-perl-1.05+
	tar xvzf Font-TTF-1.05.tar.gz
	cp -r Font-TTF-1.05 libfont-ttf-perl
	tar cvzf libfont-ttf-perl_1.05+.orig.tar.gz libfont-ttf-perl
	cp -r libfont-ttf-perl libfont-ttf-perl-1.05+
	cp -a debian libfont-ttf-perl-1.05+
	cd libfont-ttf-perl-1.05+ && dch -v 1.05+ "interim dev release" && dpkg-buildpackage -S

# make deb builds an interim deb from svn source for release
deb: deb-base
	mkdir -p dists/bionic	
	dch -D bionic -v 1.05+-1 -m -b -c libfont-ttf-perl-1.05+/debian/changelog "Auto build from perl for bionic"
	cd libfont-ttf-perl-1.05+ && pdebuild --buildresult ../dists/bionic -- --basetgz /var/cache/pbuilder/base-bionic.tgz 


