		Makefile.PL DIR="" \
		MAKEFILE=$(MAKE_APERL_FILE) LINKTYPE=static \
		MAKEAPERL=1 NORECURS=1 CCCDLFLAGS= \
		INSTALL_BASE=/home/barry/perl5 \
		LIB=/home/barry/Desktop/Owen_Donohue/mirdeep2/mirdeep2-master/lib/perl5


