# --- MakeMaker ppd section:
# Creates a PPD (Perl Package Description) for a binary distribution.
ppd :
	$(NOECHO) $(ECHO) '<SOFTPKG NAME="Font-TTF" VERSION="1.05">' > Font-TTF.ppd
	$(NOECHO) $(ECHO) '    <ABSTRACT>TTF font support for Perl</ABSTRACT>' >> Font-TTF.ppd
	$(NOECHO) $(ECHO) '    <AUTHOR>martin_hosken@sil.org</AUTHOR>' >> Font-TTF.ppd
	$(NOECHO) $(ECHO) '    <IMPLEMENTATION>' >> Font-TTF.ppd
	$(NOECHO) $(ECHO) '        <REQUIRE NAME="IO::String" />' >> Font-TTF.ppd
	$(NOECHO) $(ECHO) '        <ARCHITECTURE NAME="x86_64-linux-gnu-thread-multi-5.26" />' >> Font-TTF.ppd
	$(NOECHO) $(ECHO) '        <CODEBASE HREF="" />' >> Font-TTF.ppd
	$(NOECHO) $(ECHO) '    </IMPLEMENTATION>' >> Font-TTF.ppd
	$(NOECHO) $(ECHO) '</SOFTPKG>' >> Font-TTF.ppd


